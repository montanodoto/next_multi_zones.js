{
  "version": 2,
  "build": {
    "env": {
      "BUILDING_FOR_NOW": "true"
    }
  },
  "builds": [
    { "src": "blog/package.json", "use": "@now/next" },
    { "src": "home/package.json", "use": "@now/next" }
  ],
  "routes": [
    { "src": "/photo-editor/_next(.*)", "dest": "landings/_next$1" },
    { "src": "/photo-editor(.*)", "dest": "landings/blog$1" },
    { "src": "(.*)", "dest": "tech-seo$1" }
  ]
}
